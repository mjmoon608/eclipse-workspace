package Prac;

import java.util.Scanner;

class WhatDo{
	Scanner input = new Scanner(System.in);
	int whatDo = input.nextInt();
	
	protected WhatDo() {
		switch(whatDo) {
		case 1:
			System.out.println(whatDo);
		case 2:
			System.out.println(whatDo);
		}
			
		
	}
}


