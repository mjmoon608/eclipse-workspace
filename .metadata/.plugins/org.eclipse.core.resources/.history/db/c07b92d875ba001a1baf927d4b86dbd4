public class Item {
	Map<String, Integer> characterSkill = new HashMap<String, Integer>();
	
	public Item() {
		characterSkill.put("³ª¹µ °¡Áö", 0);
		characterSkill.put("´ŞÆØÀÌ ²®Áú", 0);
		characterSkill.put("µ·", 0);
		
		
